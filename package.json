{
  "name": "@froglet/c3-components",
  "version": "0.0.1-dev",
  "description": "C3 Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "unpkg": "dist/c3-components/c3-components.esm.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "format.tokens": "style-dictionary build --config tokens/style-dictionary.config.json",
    "generate": "stencil generate",
    "preinstall": "npx check-node-version --node $(cat .nvmrc)",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0"
  },
  "devDependencies": {
    "@stencil/postcss": "^2.1.0",
    "@types/jest": "^27.0.3",
    "classnames": "^2.3.1",
    "concurrently": "^7.1.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "lit-html": "^2.2.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.4",
    "puppeteer": "^10.0.0",
    "style-dictionary": "^3.7.0",
    "wait-on": "^6.0.1",
    "web-component-analyzer": "^2.0.0-next.4"
  },
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs"
}
